{
    "editor.tabSize": 4,
    "files.exclude": {
        "**/dist-newstyle": true,
        "**/target": true,
        "bazel-*": true,
    },
    "rust.clippy_preference": "on",
    "C_Cpp.codeAnalysis.clangTidy.enabled": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.renderFinalNewline": "dimmed",
    "files.associations": {
        "Jenkinsfile*": "groovy",
        "string": "cpp",
        "string_view": "cpp",
        "__locale": "cpp",
        "ios": "cpp",
        "stdexcept": "cpp",
        "vector": "cpp"
    },
}
